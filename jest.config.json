{
  "testEnvironment": "node",
  "testMatch": ["**/tests/**/*.test.js"],
  "collectCoverageFrom": [
    "routes/**/*.js",
    "utils/**/*.js",
    "middleware/**/*.js",
    "config/**/*.js",
    "!**/node_modules/**"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 70,
      "functions": 70,
      "lines": 70,
      "statements": 70
    }
  }
}