{
  "name": "bam-backend",
  "version": "1.0.0",
  "description": "Backend de l'application BAM Bouteille A la Mer",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "node scripts/integration-tests.js",
    "test:unit": "jest --testMatch=\"**/tests/**/*.test.js\" --testPathIgnorePatterns=\"integration\"",
    "test:all": "npm run test:unit && npm run test:integration",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:seed": "node prisma/seed.js",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loutxo/bam-backend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Loutxo/bam-backend/issues"
  },
  "homepage": "https://github.com/Loutxo/bam-backend#readme",
  "dependencies": {
    "@prisma/client": "^6.13.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.8.4",
    "@react-navigation/native": "^7.1.19",
    "@react-navigation/stack": "^7.6.3",
    "@supabase/supabase-js": "^2.80.0",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.8.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "expo-camera": "^17.0.9",
    "expo-constants": "^18.0.10",
    "expo-image-picker": "^17.0.8",
    "expo-location": "^19.0.7",
    "expo-notifications": "^0.32.12",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "firebase-admin": "^13.5.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "prisma": "^6.13.0",
    "react-hook-form": "^7.66.0",
    "react-native-maps": "^1.26.18",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.4"
  }
}
